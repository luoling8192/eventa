{
  "name": "@unbird/eventa",
  "type": "module",
  "version": "0.0.3",
  "packageManager": "pnpm@10.13.1",
  "description": "Events are all you need",
  "author": {
    "name": "RainbowBird",
    "email": "rbxin2003@outlook.com",
    "url": "https://github.com/luoling8192"
  },
  "license": "MIT",
  "homepage": "https://github.com/luoling8192/eventa",
  "repository": {
    "type": "git",
    "url": "https://github.com/luoling8192/eventa.git"
  },
  "exports": {
    ".": {
      "type": "./dist/index.mts",
      "default": "./dist/index.mjs"
    },
    "./adapters/event-emitter": {
      "type": "./dist/adapters/event-emitter/index.mts",
      "default": "./dist/adapters/event-emitter/index.mjs"
    },
    "./adapters/event-target": {
      "type": "./dist/adapters/event-target/index.mts",
      "default": "./dist/adapters/event-target/index.mjs"
    },
    "./adapters/webworkers/worker": {
      "type": "./dist/adapters/webworkers/worker/index.mts",
      "default": "./dist/adapters/webworkers/worker/index.mjs"
    },
    "./adapters/webworkers": {
      "type": "./dist/adapters/webworkers/index.mts",
      "default": "./dist/adapters/webworkers/index.mjs"
    },
    "./adapters/websocket/browser": {
      "type": "./dist/adapters/websocket/browser/index.mts",
      "default": "./dist/adapters/websocket/browser/index.mjs"
    },
    "./adapters/websocket/h3": {
      "type": "./dist/adapters/websocket/h3/index.mts",
      "default": "./dist/adapters/websocket/h3/index.mjs"
    },
    "./adapters/websocket": {
      "type": "./dist/adapters/websocket/index.mts",
      "default": "./dist/adapters/websocket/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "README.md",
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "tsdown",
    "test": "vitest --coverage",
    "test:run": "vitest run",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache --fix .",
    "typecheck": "tsc --noEmit",
    "up": "taze -w -r -I -f && pnpm prune && pnpm dedupe"
  },
  "peerDependencies": {
    "electron": ">=30",
    "h3": "2.0.0-beta.1",
    "web-worker": "^1.5.0"
  },
  "peerDependenciesMeta": {
    "electron": {
      "optional": true
    },
    "h3": {
      "optional": true
    },
    "web-worker": {
      "optional": true
    }
  },
  "dependencies": {
    "nanoid": "^5.1.5",
    "picomatch": "^4.0.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.0.0",
    "@types/node": "^24.1.0",
    "@vitest/web-worker": "^3.2.4",
    "bumpp": "^10.2.0",
    "crossws": "^0.4.1",
    "eslint": "^9.32.0",
    "taze": "^19.1.0",
    "tsdown": "^0.13.0",
    "typescript": "^5.8.3",
    "unplugin-unused": "^0.5.1",
    "vite": "^7.0.6",
    "vitest": "^3.2.4"
  }
}
