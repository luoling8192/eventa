{
  "name": "eventa",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@10.13.1",
  "description": "Events are all you need",
  "author": {
    "name": "RainbowBird",
    "email": "rbxin2003@outlook.com",
    "url": "https://github.com/luoling8192"
  },
  "license": "MIT",
  "homepage": "https://github.com/luoling8192/eventa",
  "repository": {
    "type": "git",
    "url": "https://github.com/luoling8192/eventa.git"
  },
  "exports": {
    ".": {
      "type": "./dist/index.mts",
      "default": "./dist/index.mjs"
    },
    "./ws-adapters/browser/index": {
      "type": "./dist/ws-adapters/browser/index.mts",
      "default": "./dist/ws-adapters/browser/index.mjs"
    },
    "./ws-adapters/h3/index": {
      "type": "./dist/ws-adapters/h3/index.mts",
      "default": "./dist/ws-adapters/h3/index.mjs"
    },
    "./ws-adapters/index": {
      "type": "./dist/ws-adapters/index.mts",
      "default": "./dist/ws-adapters/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "README.md",
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "tsdown",
    "test": "vitest --coverage",
    "test:run": "vitest run",
    "lint": "eslint --cache .",
    "typecheck": "tsc --noEmit",
    "up": "taze -w -r -I -f && pnpm prune && pnpm dedupe"
  },
  "peerDependencies": {
    "h3": ">=1"
  },
  "peerDependenciesMeta": {
    "h3": {
      "optional": true
    }
  },
  "dependencies": {
    "nanoid": "^5.1.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.0.0",
    "@types/node": "^24.1.0",
    "bumpp": "^10.2.0",
    "eslint": "^9.32.0",
    "taze": "^19.1.0",
    "tsdown": "^0.13.0",
    "typescript": "^5.8.3",
    "unplugin-unused": "^0.5.1",
    "vitest": "^3.2.4"
  }
}
